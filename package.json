{
  "name": "hun-bot-three",
  "version": "1.0.0",
  "description": "Three.js portfolio with immersive 3D experiences",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\""
  },
  "keywords": [
    "threejs",
    "portfolio",
    "3d",
    "webgl",
    "vite",
    "typescript"
  ],
  "author": "Jeonghun",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/three": "^0.181.0",
    "gsap": "^3.13.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12"
  }
}
